<MainDashboardPage>:
    size_hint:1,1
    orientation:'vertical'
    BoxLayout:
        size_hint:1,None
        height:34
        orientation:'horizontal'
        canvas.before:
            Color:
                rgba:(0.294, 0.525, 0.451, 1)
            Rectangle:
                pos:self.pos
                size:self.size

        IconClick:
            id      :btn_back
            args    :{'normal':(0.275, 0.639, 0.482, 1), 'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[2]}
            icon_source:'arrow.png'
            size_hint_y:1
            size_hint_x:None
            width:30
            on_press:root.call_login_form()

        Label:
            text:'Welcome'
            font_size:18
            bold:True
            text_size:self.size
            valign:'center'
            halign:'center'

        IconClick:
            size_hint_x:None
            width:30

    BoxLayout:
        id:working_area
        size_hint:1,1
        padding:0
        canvas.before:
            Color:
                # rgba:(0.294, 0.525, 0.451,1)
                rgba:(0.263, 0.227, 0.4,1)
            Rectangle:
                pos:self.pos
                size:self.size
        BoxLayout:
            id:welcome_page
            size_hint:1,1
            orientation:'vertical'
            padding:15
            spacing:10
            Label:
                id:lbl_msg
                text:''
                size_hint:1,None
                height:60
                color:(0.922, 0.329, 0.392,1)

            BoxLayout:
                spacing: 10
                padding: 15, 15,15,0
                RoundedButton:
                    id      :btn_checkin
                    args    :{'normal':(0.275, 0.639, 0.482, 1),'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                    text    :"Check In"
                    bold    :True
                    padding : 10
                    font_size:14
                    size_hint_y:1
                    on_press:root.checkin_analyser()


                RoundedButton:
                    id      :btn_checkout
                    args    :{'normal':(0.275, 0.639, 0.482, 1),'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                    text    :"Check Out"
                    bold    :True
                    font_size:14
                    size_hint_y:1
                    on_press:root.checkout_analyser()

            BoxLayout:
                padding: 15
                spacing: 10
                RoundedButton:
                    id      :btn_pause
                    args    :{'normal':(0.922, 0.329, 0.392, 1), 'down':(0.878, 0.741, 0.192,1), 'size': self.size, 'anchor_x':'center', 'anchor_y': 'center', 'pos':self.pos, 'radius':[7]}
                    text    :"Pause Notification"
                    bold    :True
                    font_size:14
                    size_hint_y:1
                    on_press:root.call_pause_modal()

            RoundedButton:
                id      :btn_motivational_task
                args    :{'normal':(0.878, 0.741, 0.192, 1),'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                text    :"Motivational Task"
                bold    :True
                font_size:14
                size_hint_y:1
                on_press:root.call_motivational_task_modal()

            RoundedButton:
                args    :{'normal':(0.8, 0.592, 0.322, 1),'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                text    :"View Graph"
                bold    :True
                font_size:14
                size_hint_y:1
                on_press:root.call_graph_modal()

            RoundedButton:
                id      :btn_motivational_task
                args    :{'normal':(0.275, 0.639, 0.482, 1),'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                text    :"Setting"
                bold    :True
                font_size:14
                size_hint_y:1
                on_press:root.call_settings()



<MotivationManagerModel>:
    size_hint:1,1
    size_hint: 1.0, 0.6
    auto_dismiss:False
    id:popup_modal
    orientation: "vertical"
    padding:2
    canvas.before:
        Color:
            rgba:(.9,.9,.9,.9)
        Rectangle:
            pos:self.pos
            size:self.size
    BoxLayout:
        id:working_section
        orientation: "vertical"
        size_hint_x:1
        size_hint_y:1
        padding:3
        spacing:5
        canvas.before:
            Color:
                rgba:(0.9,0.9,0.9,1)
            Rectangle:
                pos:self.pos
                size:self.size

        BoxLayout:
            size_hint:1,None
            height:34
            orientation:'horizontal'
            canvas.before:
                Color:
                    rgba:(0.275, 0.639, 0.482, 1)
                Rectangle:
                    pos:self.pos
                    size:self.size
            Label:
                text:'Motivation Task Manager'
                font_size:18
                bold:True
                text_size:self.size
                valign:'center'
                halign:'center'

        Label:
            id:task_msg
            text:""
            text_size:self.size
            halign:"center"
            valign:"center"
            font_size:15
            color:(0.922, 0.329, 0.392,1)
            size_hint:1,None
            height:40
        BoxLayout:
            orientation:"vertical"
            size_hint:1,1
            padding:0.5
            Label:
                id:lbl_sitting
                text:"Motivation For Sitting:"
                size_hint:1,1
                font_size:15
                text_size:self.size
                halign:'left'
                valign:'center'
                color:(0,0,0,1)
            BoxLayout:
                size_hint:1,None
                height:33
                padding:0
                canvas.before:
                    Color:
                        rgba:(1,1,1,1)
                    RoundedRectangle:
                        pos:self.pos
                        size:self.size
                        radius:[4.5]
                TextInput:
                    id:sitting
                    text:''
                    multiline:False
                    background_color: 0,0,0,0
                    foreground_color: 0,0,0,1
                    font_size:15
            Label:
                id:lbl_walking
                text:"Motivation For Walking:"
                size_hint:1,1
                font_size:15
                text_size:self.size
                halign:'left'
                valign:'center'
                color:(0,0,0,1)
            BoxLayout:
                size_hint:1,None
                height:33
                padding:0
                canvas.before:
                    Color:
                        rgba:(1,1,1,1)
                    RoundedRectangle:
                        pos:self.pos
                        size:self.size
                        radius:[4.5]
                TextInput:
                    id:walking
                    text:''
                    multiline:False
                    background_color: 0,0,0,0
                    foreground_color: 0,0,0,1
                    font_size:15
            Label:
                id:lbl_running
                text:"Motivation For Running:"
                size_hint:1,1
                font_size:15
                text_size:self.size
                halign:'left'
                valign:'center'
                color:(0,0,0,1)
            BoxLayout:
                size_hint:1,None
                height:33
                padding:0
                canvas.before:
                    Color:
                        rgba:(1,1,1,1)
                    RoundedRectangle:
                        pos:self.pos
                        size:self.size
                        radius:[2.5]
                TextInput:
                    id:running
                    text:''
                    multiline:False
                    background_color: 0,0,0,0
                    foreground_color: 0,0,0,1
                    font_size:15
            Label:
                id:lbl_continuous
                text:"Motivation to do movement:"
                size_hint:1,1
                font_size:15
                text_size:self.size
                halign:'left'
                valign:'center'
                color:(0,0,0,1)
            BoxLayout:
                size_hint:1,None
                height:33
                padding:0
                canvas.before:
                    Color:
                        rgba:(1,1,1,1)
                    RoundedRectangle:
                        pos:self.pos
                        size:self.size
                        radius:[4.5]
                TextInput:
                    id:continuous
                    text:''
                    multiline:False
                    background_color: 0,0,0,0
                    foreground_color: 0,0,0,1
                    font_size:15

        BoxLayout:
            orientation:"horizontal"
            size_hint:1,None
            height:40
            spacing:2
            padding:2
            RoundedButton:
                id      :btn_save
                args    :{'normal':(0.275, 0.639, 0.482, 1), 'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                text    :"Save"
                bold    :True
                font_size:15
                size_hint_y:None
                height:35
                on_release:root.save_tasks(self.text, sitting.text, walking.text, running.text)
            RoundedButton:
                id      :btn_close
                args    :{'normal':(0.275, 0.639, 0.482, 1), 'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                text    :"Close"
                bold    :True
                font_size:15
                color:(0.545, 0, 0, 1)
                size_hint_y:None
                height:35
                on_press:root.close_model()


