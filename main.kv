#:import utils kivy.utils

<IconClick>:
    background_color: 0,0,0,0
    orientation     : 'vertical'
    padding         : 1
    color           : (1, 1, 1, 1)
    size_hint       : 1,1
    on_state        : root.on_click(self.state)


<RoundedButton>:
    background_color: 0,0,0,0
    orientation     : 'vertical'
    padding         : 1
    color           : (1, 1, 1, 1)
    size_hint       :1,1
    on_state        : root.on_click(self.state)

    Label:
        id:lbl
        size_hint_y:.01



<UserLoginPage>:
    size_hint:1,1
    orientation:'vertical'
    BoxLayout:
        size_hint:1,None
        height:34
        orientation:'horizontal'
        canvas.before:
            Color:
                rgba:(0.275, 0.639, 0.482, 1)
            Rectangle:
                pos:self.pos
                size:self.size

        Label:
            text:'Welcome to the App!'
            font_size:18
            bold:True
            text_size:self.size
            valign:'center'
            halign:'center'

    BoxLayout:
        id:working_area
        size_hint:1,1
        padding:2
        canvas.before:
            Color:
                rgba:(.9,.9,.9,1)
            Rectangle:
                pos:self.pos
                size:self.size
        BoxLayout:
            id:welcome_page
            size_hint:1,1
            orientation:'vertical'
            padding:15
            spacing:5
            BoxLayout:
                size_hint:1,0.2
                orientation:"vertical"
                # height:40

                Label:
                    id:login_heading
                    text:" Please login first"
                    font_size:30
                    bold:True
                    text_size:self.size
                    halign:'center'
                    valign:'top'
                    color:(0.275, 0.639, 0.482, 1)
                Label:
                    id:lbl_msg
                    text:""
                    font_size:12
                    text_size:self.size
                    halign:'center'
                    valign:'center'
                    color:(0.396, 0.6, 1,1)
            BoxLayout:
                size_hint:1,0.55
                # height:0
                padding:0
                spacing:2
                orientation:"vertical"
                Label:
                    id:lbl_username
                    text:"Username:"
                    size_hint:1,1
                    font_size:17
                    text_size:self.size
                    halign:'left'
                    valign:'center'
                    color:(0, 0.431, 0.498, 1)
                BoxLayout:
                    size_hint:1,None
                    height:33
                    padding:0
                    canvas.before:
                        Color:
                            rgba:(1,1,1,1)
                        RoundedRectangle:
                            pos:self.pos
                            size:self.size
                            radius:[4.5]
                    TextInput:
                        id  : username
                        text: ''
                        multiline:False
                        background_color: 0,0,0,0
                        foreground_color: 0,0,0,1
                        font_size:15

                Label:
                    id:lbl_password
                    text:"Password:"
                    size_hint:1,1
                    font_size:17
                    text_size:self.size
                    halign:'left'
                    valign:'center'
                    color:(0, 0.431, 0.498, 1)
                BoxLayout:
                    size_hint:1,None
                    height:33
                    padding:0
                    canvas.before:
                        Color:
                            rgba:(1,1,1,1)
                        RoundedRectangle:
                            pos:self.pos
                            size:self.size
                            radius:[4.5]
                    TextInput:
                        id  : password
                        text: ''
                        multiline:False
                        password:True
                        background_color: 0,0,0,0
                        foreground_color: 0,0,0,1
                        font_size:15
                Label:
                    text:''
                    size_hint:1,None
                    height:40
                RoundedButton:
                    id      :btn_login
                    args    :{'normal':(0.275, 0.639, 0.482, 1), 'down':(0.878, 0.741, 0.192,1), 'size':self.size, 'pos':self.pos, 'radius':[7]}
                    text    :"Login"
                    bold    :True
                    font_size:15
                    size_hint_y:None
                    height:35
                    on_press:root.login(username.text, password.text)
                BoxLayout:
                    orientation:'horizontal'
                    size_hint:1,None
                    height:30
                    Label:
                        text:"First time to our app?"
                        size_hint:1,1
                        color:(0,0,0,1)
                    LabelButton:
                        id:btn_register
                        text:"Register"
                        on_press:root.call_signup();

            BoxLayout:
                size_hint: 1,.30


